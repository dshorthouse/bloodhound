%div{class:"p-2"}
  %form{id:"form-org-wrapper", method: "get", action: "/agents", "accept-charset" => "UTF-8", class: "form-inline helpers"}
    %div{class: "form-group w-75"}
      %input{class:"typeahead search-agent", id:"typeahead-agent", type: :text, name: "q", placeholder: "Daniel H. Janzen", value: "#{params[:q]}", size:"25"}
    %input{type: :submit, class:"btn btn-success medium-margins", value:"Search"}